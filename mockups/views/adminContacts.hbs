<div class="container">
    <div class="row">
        <div class="list-goup col-md-6">
            <div class="list-group-item active">
                {{#each users}}
                    {{#if isContact}}
                        <a href="/deleteContact/{{../u}}/{{user}}" class="btn btn-danger">{{user}}</a>
                    {{else}}
                        <a href="/addContact/{{../u}}/{{user}}" class="btn btn-success">{{user}}</a>
                    {{/if}}
                {{/each}}
            </div>
        </div>
    </div>
</div>
